<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- primary color -->
    <SolidColorBrush x:Key="PrimaryColor" Color="#82BBB2"/>
    <SolidColorBrush x:Key="MainBackgroundColor" Color="#F0FFF0"></SolidColorBrush>
    <SolidColorBrush x:Key="LightBlackColor" Color="#49454F"></SolidColorBrush>
    <SolidColorBrush x:Key="White" Color="#FFF"></SolidColorBrush>
    <SolidColorBrush x:Key="Orange" Color="#FF7222"></SolidColorBrush>
    <SolidColorBrush x:Key="BlueSky" Color="#3081D0"></SolidColorBrush>
</ResourceDictionary>